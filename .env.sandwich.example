# ========================================
# 샌드위치 전략 환경변수 설정
# ========================================

# 기본 설정
SANDWICH_MIN_VALUE_ETH=0.1                    # 최소 거래 가치 (ETH)
SANDWICH_MAX_GAS_PRICE_GWEI=200               # 최대 가스 가격 (Gwei)
SANDWICH_MIN_PROFIT_ETH=0.01                  # 최소 수익 (ETH)
SANDWICH_MIN_PROFIT_PERCENTAGE=0.02           # 최소 수익률 (2%)
SANDWICH_MAX_PRICE_IMPACT=0.05                # 최대 가격 영향 (5%)
SANDWICH_KELLY_RISK_FACTOR=0.5                # Kelly 위험 계수 (Half Kelly)

# 컨트랙트 설정
SANDWICH_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000  # 샌드위치 컨트랙트 주소
SANDWICH_FLASHBOTS_RELAY_URL=https://relay.flashbots.net              # Flashbots Relay URL

# 가스 설정
SANDWICH_GAS_LIMIT=200000                     # 트랜잭션 가스 한도
SANDWICH_GAS_PER_TX=200000                    # 각 트랜잭션당 가스 사용량
SANDWICH_FRONT_RUN_PRIORITY_FEE_GWEI=5        # Front-run 우선순위 수수료 (Gwei)
SANDWICH_BACK_RUN_PRIORITY_FEE_GWEI=2         # Back-run 우선순위 수수료 (Gwei)

# 경쟁 수준별 우선순위 수수료 (Gwei)
SANDWICH_PRIORITY_FEE_LOW_GWEI=1              # 낮은 경쟁
SANDWICH_PRIORITY_FEE_MEDIUM_GWEI=2           # 중간 경쟁
SANDWICH_PRIORITY_FEE_HIGH_GWEI=5             # 높은 경쟁
SANDWICH_PRIORITY_FEE_CRITICAL_GWEI=10        # 치열한 경쟁

# DEX 수수료 설정
SANDWICH_UNISWAP_V2_FEE=0.003                 # Uniswap V2 수수료 (0.3%)
SANDWICH_UNISWAP_V3_FEE=0.003                 # Uniswap V3 수수료 (0.3%)
SANDWICH_DEFAULT_FEE=0.003                    # 기본 DEX 수수료 (0.3%)
SANDWICH_UNISWAP_V3_FEE_TIER=3000             # Uniswap V3 수수료 티어 (0.3%)

# 타이밍 설정
SANDWICH_DEADLINE_SECS=300                    # 트랜잭션 데드라인 (초)
SANDWICH_MAX_WAIT_BLOCKS=3                    # 번들 포함 최대 대기 블록 수
SANDWICH_WAIT_SECONDS=3                       # 블록 확인 대기 시간 (초)
SANDWICH_STATS_INTERVAL_SECS=60               # 통계 출력 간격 (초)

# ========================================
# 사용법:
# 1. 이 파일을 .env로 복사
# 2. 실제 값으로 수정
# 3. 환경변수 로드: source .env
# ========================================